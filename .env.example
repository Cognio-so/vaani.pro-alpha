# API Keys
GROQ_API_KEY=your_groq_api_key_here
REPLICATE_API_KEY=your_replicate_api_key_here
REPLICATE_API_TOKEN=your_replicate_api_key_here  # Same as REPLICATE_API_KEY
TAVILY_API_KEY=your_tavily_api_key_here

# Configuration
DEFAULT_MODEL=llama-3.3-70b-versatile
LIGHTWEIGHT_MODEL=Llama-3.1-8b-instant
IMAGE_MODEL=black-forest-labs/flux-schnell

# Authentication
AUTH_TOKEN=your_auth_token_here

# Database
DB_PATH=vaani_state.db

# Feature Flags
WEB_SEARCH_ENABLED=true
DEEP_RESEARCH_ENABLED=false

# Note: Replicate requires the API key to be set as REPLICATE_API_TOKEN in the environment
# Both REPLICATE_API_KEY and REPLICATE_API_TOKEN are included for clarity, but only REPLICATE_API_TOKEN is used by the library 